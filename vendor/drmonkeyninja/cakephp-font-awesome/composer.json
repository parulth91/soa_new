{
    "name": "drmonkeyninja/cakephp-font-awesome",
    "description": "CakePHP helper for creating Font Awesome links",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "helper"],
    "homepage": "https://github.com/drmonkeyninja/cakephp-font-awesome",
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Carter",
            "role": "Author"
        }
    ],
    "require": {
        "cakephp/cakephp": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "FontAwesome\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "FontAwesome\\Test\\": "tests"
        }
    }
}